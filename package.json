{
    "name": "tradestats-web",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "build:staging": "tsc && vite build --mode staging",
        "serve": "firebase emulators:start --import=./data --export-on-exit data",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 50",
        "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 50 --fix",
        "deploy": "firebase deploy --only hosting",
        "deploy:staging": "firebase deploy --only hosting:staging-tradestats-web",
        "preview": "vite preview"
    },
    "dependencies": {
        "@emotion/react": "^11.11.3",
        "@emotion/styled": "^11.11.0",
        "@fullcalendar/core": "^6.1.10",
        "@fullcalendar/daygrid": "^6.1.10",
        "@fullcalendar/interaction": "^6.1.10",
        "@fullcalendar/list": "^6.1.10",
        "@fullcalendar/react": "^6.1.10",
        "@fullcalendar/timegrid": "^6.1.10",
        "@mui/icons-material": "^5.15.3",
        "@mui/material": "^5.15.3",
        "@mui/x-data-grid": "^6.18.7",
        "@mui/x-data-grid-generator": "^6.19.1",
        "@nivo/bar": "^0.84.0",
        "@nivo/core": "^0.84.0",
        "@nivo/line": "^0.84.0",
        "@nivo/pie": "^0.84.0",
        "dotenv": "^16.3.1",
        "firebase": "^10.6.0",
        "firebase-admin": "^11.11.1",
        "formik": "^2.4.5",
        "moment": "^2.30.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-pro-sidebar": "^0.7.1",
        "react-router-dom": "^6.21.1",
        "typescript-plugin-css-modules": "^5.0.2",
        "yup": "^1.3.3"
    },
    "devDependencies": {
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.10.0",
        "@vitejs/plugin-react": "^4.2.0",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.57.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.4",
        "postcss": "^8.4.31",
        "tailwindcss": "^3.3.5",
        "typescript": "^5.2.2",
        "vite": "^5.0.0"
    },
    "volta": {
        "node": "20.12.2"
    }
}